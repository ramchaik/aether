    "@grpc/proto-loader": "^0.7.13",
    "@sinclair/typebox": "^0.32.34",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "nanoid": "^5.0.7",
    "pg": "^8.12.0",
